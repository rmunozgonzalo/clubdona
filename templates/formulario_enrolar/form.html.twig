      <script type="text/javascript">
      
      $(document).ready(function() {
         $("#phone").val("+569")
         var now = new Date();

         var day = ("0" + now.getDate()).slice(-2);
         var month = ("0" + (now.getMonth() + 1)).slice(-2);

         var today = now.getFullYear()+"-"+(month)+"-"+(day) ;
         $('#birthday').val(today);
         console.log(today)
         $('#enrolarForm').on('submit', function(e) { 
               e.preventDefault();  
               var dataForm = $("#enrolarForm :input").serializeArray();
               console.log(dataForm); 

               url = $("#facebook").val();
               var pattern = /^(?:(?:http|https):\/\/)?(?:www.)?facebook.com\/(?:(?:\w)*#!\/)?(?:pages\/)?(?:[?\w\-]*\/)?(?:profile.php\?id=(?=\d.*))?([\w\-]*)?$/;
               var regexNumero = /^(\+?56)?(\s?)(0?9)(\s?)[9876543]\d{7}$/g;
               var phone = $("#phone").val()
               validacionPhone = true
               validacionUrl = true
               if(phone.length>4){
                  validacionPhone = regexNumero.test(phone) ? true:false;
               }
               if(url.length>0){
                  validacionUrl = pattern.test(url) ? true:false;
               }
               if(validacionUrl && validacionPhone) {
                     $.ajax({
                        url : "{{ path('enrolar_guardar') }}",
                        type: "POST",
                        data: dataForm,
                        success : function (data) {

                           if(data.status){
                              Swal.fire(data.message)
                              $('#enrolarForm').trigger("reset");
                              $('#birthday').val(today);
                              $("#phone").val("+569")

                           }
                           else{
                              Swal.fire(data.message)
                           }
                        },
                        error: function () {
                           Swal.fire('Ha ocurrido un error con tu registro, intenta más tarde')
                        }
                     });
               }
               else {
                     if(!validacionUrl)Swal.fire('La url de facebook no es válida')
                     if(!validacionPhone)Swal.fire('El número no es válido')
               }
            });
            
            var regexNumero = /^(\+?56)?(\s?)(0?9)(\s?)[9876543]\d{7}$/g;
            $("#phone").keyup(function(){
               if($("#phone").val() == ""){
                  $("#phone").val("+569")
               }
            });

         });
      </script>
